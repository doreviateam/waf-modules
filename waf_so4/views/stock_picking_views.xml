<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_form_inherit_waf_so3" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit.waf.so3</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">            

            <notebook position="inside">
                <page string="Informations de livraison" name="delivery_info">
                    <group>
                        <group string="Stakeholders" name="stakeholders">
                            <field name="stakeholder_id"/>
                            <field name="delivery_address_id"/>
                            <field name="mandator_id" readonly="1"/>
                        </group>
                        <group string="Delivery Address" name="delivery_address">
                            <field name="delivery_site_name" readonly="1"/>
                            <field name="delivery_street" readonly="1"/>
                            <field name="delivery_street2" readonly="1"/>
                            <field name="delivery_city" readonly="1"/>
                            <field name="delivery_zip" readonly="1"/>
                        </group>
                    </group>
                </page>
            </notebook>
            
        </field>
    </record>
</odoo> 