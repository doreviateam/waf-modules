<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_sale_line_dispatch" model="ir.sequence">
            <field name="name">Dispatch Line Sequence</field>
            <field name="code">sale.line.dispatch</field>
            <field name="prefix">DISP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_sale_dispatch" model="ir.sequence">
            <field name="name">Dispatch Sequence</field>
            <field name="code">sale.dispatch</field>
            <field name="prefix">DSP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_sale_dispatch_group" model="ir.sequence">
            <field name="name">Groupe de Dispatch</field>
            <field name="code">sale.dispatch.group</field>
            <field name="prefix">GRP/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>