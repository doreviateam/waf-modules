<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_form_view_inherit_dispatch" model="ir.ui.view">
        <field name="name">product.template.form.inherit.dispatch</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='type']" position="after">
                <field name="dispatchable" widget="boolean_toggle"/>
            </xpath>
        </field>
    </record>
</odoo> 