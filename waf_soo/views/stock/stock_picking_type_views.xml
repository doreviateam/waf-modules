<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_type_form_inherit_waf_soo" model="ir.ui.view">
        <field name="name">stock.picking.type.form.inherit.waf.soo</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Configuration WAF" name="waf_config">
                    <group>
                        <field name="use_existing_lots" string="Utiliser les lots existants"/>
                    </group>
                </page>
            </notebook>
        </field>
    </record>
</odoo> 