<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Adresse Bar De La Rotonde -->
        <record id="partner_address_bar_rotonde" model="partner.address">
            <field name="name">Bar De La Rotonde</field>
            <field name="street">2 rue de la basse mer</field>
            <field name="street2">Impasse des landais</field>
            <field name="zip">34010</field>
            <field name="city">Montpellier</field>
            <field name="country_id" ref="base.fr"/>
            <field name="type">delivery</field>
            <field name="active" eval="True"/>
            <field name="partner_ids" eval="[(4, ref('partner_cm_antigone'))]"/>
        </record>

    </data>
</odoo>
