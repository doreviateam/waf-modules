<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Groupement CMH34 Monétique -->
        <record id="groupment_cmh34_monetique" model="partner.groupment">
            <field name="name">CMH34 Monétique</field>
            <field name="code">CMH34-MLS</field>
            <field name="agent_id" ref="partner_cm_herault"/>
            <field name="interest_id" ref="interest_monetique"/>
            <field name="member_ids" eval="[
                (6, 0, [
                    ref('partner_cm_antigone'),
                    ref('partner_cm_gambetta'),
                    ref('partner_cm_beziers')
                ])
            ]"/>
            <field name="date_start" eval="time.strftime('%Y-%m-%d')"/>
            <field name="active" eval="True"/>
            <field name="notes">Groupement des agences CM34 pour les services monétiques et logistiques</field>
        </record>
    </data>
</odoo> 