<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Définition des catégories -->
        <record id="partner_interest_category_monetique" model="partner.interest.category">
            <field name="name">Monétique</field>
            <field name="sequence">10</field>
        </record>

        <!-- Définition des centres d'intérêt -->
        <record id="partner_interest_mls" model="partner.interest">
            <field name="name">Monétique Logistique &amp; Services</field>
            <field name="code">MLS</field>
            <field name="category_id" ref="partner_interest_category_monetique"/>
            <field name="sequence">10</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
    </data>
</odoo>
