<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="partner_interest_category_view_tree" model="ir.ui.view">
        <field name="name">partner.interest.category.tree</field>
        <field name="model">partner.interest.category</field>
        <field name="arch" type="xml">
            <tree>
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="company_id" groups="base.group_multi_company"/>
            </tree>
        </field>
    </record>

    <record id="partner_interest_category_view_form" model="ir.ui.view">
        <field name="name">partner.interest.category.form</field>
        <field name="model">partner.interest.category</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <h1><field name="name"/></h1>
                    </div>
                    <group>
                        <field name="sequence"/>
                        <field name="company_id" groups="base.group_multi_company"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="partner_interest_category_view_search" model="ir.ui.view">
        <field name="name">partner.interest.category.search</field>
        <field name="model">partner.interest.category</field>
        <field name="arch" type="xml">
            <search>
                <field name="name"/>
                <field name="company_id" groups="base.group_multi_company"/>
            </search>
        </field>
    </record>
</odoo> 